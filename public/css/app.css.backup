/* Modern Admin Panel Styles - Updated Design System */

/* Use CSS variables to maintain the new color scheme */
:root {
  /* New Primary color palette */
  --primary: #7fb7f2;               /* light blue */
  --primary-light: #7fb7f2;
  --primary-dark: #7fb7f2;
  --primary-rgb: 127, 183, 242;

  /* New Secondary color palette */
  --secondary: #9bd698;             /* light green */
  --secondary-light: #9bd698;
  --secondary-dark: #9bd698;
  --secondary-rgb: 155, 214, 152;

  /* Accent color */
  --accent: #fbe17e;                /* light yellow */
  --accent-light: #fbe17e;
  --accent-dark: #fbe17e;
  --accent-rgb: 251, 225, 126;

  /* Surface colors */
  --surface: #ffffff;               /* white */
  --surface-light: #ffffff;
  --surface-dark: #7fb7f2;          /* light blue */
  --surface-rgb: 255, 255, 255;

  /* Background colors */
  --background: #ffffff;            /* white */
  --background-light: #ffffff;
  --background-dark: #7fb7f2;       /* light blue */
  --background-rgb: 255, 255, 255;

  /* Text colors */
  --text-primary: #7fb7f2;          /* light blue */
  --text-primary-light: #7fb7f2;
  --text-primary-dark: #ffffff;     /* white */
  --text-primary-rgb: 127, 183, 242;

  --text-secondary: #9bd698;        /* light green */
  --text-secondary-light: #9bd698;  /* light green */
  --text-secondary-dark: #fbe17e;   /* light yellow */
  --text-secondary-rgb: 155, 214, 152;

  /* Border colors */
  --border: #fbe17e;                /* light yellow */
  --border-light: #fbe17e;
  --border-dark: #9bd698;           /* light green */
  --border-rgb: 251, 225, 126;

  /* Warning/Alert colors */
  --warning: #fbe17e;               /* light yellow */
  --warning-light: #fbe17e;
  --warning-dark: #fbe17e;
  --warning-rgb: 251, 225, 126;

  /* Success colors */
  --success: #9bd698;               /* light green */
  --success-light: #9bd698;
  --success-dark: #9bd698;
  --success-rgb: 155, 214, 152;

  /* Error colors */
  --error: #7fb7f2;                 /* light blue */
  --error-light: #7fb7f2;
  --error-dark: #7fb7f2;
  --error-rgb: 127, 183, 242;

  /* Spacing System (8px grid) */
  --spacing-xs: 4px;
  --spacing-sm: 8px;
  --spacing-md: 16px;
  --spacing-lg: 24px;
  --spacing-xl: 32px;
  --spacing-2xl: 48px;

  /* Border Radius */
  --radius-sm: 4px;
  --radius-md: 8px;
  --radius-lg: 12px;
  --radius-full: 9999px;

  /* Typography */
  --font-family: 'Figtree', -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, 'Helvetica Neue', sans-serif;
  --font-size-h1: 2.5rem;
  --font-size-h2: 2rem;
  --font-size-h3: 1.5rem;
  --font-size-body: 1rem;
  --font-size-small: 0.875rem;
  --font-weight-h1: 800;
  --font-weight-h2: 700;
  --font-weight-h3: 600;
  --font-weight-body: 400;
  --font-weight-small: 400;
  --line-height: 1.6;

  /* Shadows */
  --shadow-sm: 0 1px 3px 0 rgba(0, 0, 0, 0.1), 0 1px 2px -1px rgba(0, 0, 0, 0.1);
  --shadow-md: 0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -2px rgba(0, 0, 0, 0.1);
  --shadow-lg: 0 10px 15px -3px rgba(0, 0, 0, 0.1), 0 4px 6px -4px rgba(0, 0, 0, 0.1);
  --shadow-hover: 0 20px 25px -5px rgba(0, 0, 0, 0.1), 0 8px 10px -6px rgba(0, 0, 0, 0.1);
}

/* Base styles */
body {
  font-family: var(--font-family);
  background-color: var(--background);
  color: var(--text-primary);
  line-height: var(--line-height);
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

/* Typography */
h1 {
  font-size: var(--font-size-h1);
  font-weight: var(--font-weight-h1);
  color: var(--text-primary);
  margin-bottom: var(--spacing-xs);
  letter-spacing: -0.025em;
}

h2 {
  font-size: var(--font-size-h2);
  font-weight: var(--font-weight-h2);
  color: var(--text-primary);
  letter-spacing: -0.025em;
}

h3 {
  font-size: var(--font-size-h3);
  font-weight: var(--font-weight-h3);
  color: var(--text-primary);
}

.text-body {
  font-size: var(--font-size-body);
  font-weight: var(--font-weight-body);
  color: var(--text-primary);
}

.text-small {
  font-size: var(--font-size-small);
  font-weight: var(--font-weight-small);
  color: var(--text-secondary);
}

.text-muted {
  color: var(--text-secondary) !important;
}

